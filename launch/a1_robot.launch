<?xml version="1.0"?>
<launch>
    <arg name="rname" default="a1"/>
    <arg name="ctrl_level" default="highlevel"/>

    <!-- LCM Server for robot comms -->
    <node pkg="unitree_legged_real" type="lcm_server" name="node_lcm_server"
        respawn="false" output="screen" args="$(arg rname) $(arg ctrl_level)" />
    <!-- Convert initialpose from amrl2dmsg from robofleet to geometry_msgs/PoseStamped -->
    <!-- <node pkg="unitree_legged_real" type="a1_950_initialpose_convert" 
        name="a1_950_initialpose_convert"/> -->
    <!-- Run the SDK -->
    <node pkg="unitree_legged_real" type="a1_driver_main" name="a1_driver_node"
        respawn="false" output="screen" args="$(arg rname) $(arg ctrl_level)" />

</launch>
